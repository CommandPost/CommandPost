    <Panels>
        <Panel type="Element-Tk">
            <Mode id="0x00010004"> <!-- FCP: Wheels -->
                <ControlBank id="Standard">
                    <Bank>
                        <Control type="Button" number="0"> <!-- Element-Tk Button A -->
